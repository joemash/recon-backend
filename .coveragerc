[run]
branch = True
source=src
omit = *migrations*,
       *urls*,
       *test*,
       *admin*,
       ./manage.py,
       ./src/config/*,
       ./src/wsgi.py,
       src/**/migrations/*,
       *__init__*,
       ./dataseeder.py,
       ./setup.py,
       ./src/common/utils/choices.py

[report]
show_missing=True
exclude_lines=pragma: no cover

[html]
directory=coverage
